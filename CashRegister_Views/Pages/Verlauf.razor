@using CashRegister.Models


<MudDataGrid Items="@_items" Filterable="true">
    <Columns>
        <Column T="Einkauf" Field="@nameof(Einkauf.Belegnummer)" />
     
        <Column T="Einkauf" Field="@nameof(Einkauf.Gesamtpreis)" Title="Gesamtpreis" />    
