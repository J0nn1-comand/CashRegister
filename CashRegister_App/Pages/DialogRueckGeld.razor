<MudDialog Style="width: 400px; height: 200px;">
    <DialogContent>
       Rueckgeld: @Rueckgeld
    </DialogContent>
    <DialogActions>
        
        <MudButton Color="Color.Primary" OnClick="CancelAll">Close All</MudButton>
    </DialogActions>
</MudDialog>

@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }
    [Parameter] public double Rueckgeld { get; set; }
    
    
    private void CancelAll()
    {
        MudDialog.CancelAll();
    }
}